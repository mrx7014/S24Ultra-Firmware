<?xml version="1.0" encoding="utf-8"?>

<!-- Feature for devices with an eUICC. -->
<permissions>
    <feature name="android.hardware.telephony.euicc" />
    <feature name="android.hardware.telephony.euicc.mep" />

    <privapp-permissions package="com.samsung.euicc">
        <permission name="android.permission.RECEIVE_BOOT_COMPLETED" />
        <permission name="android.permission.INTERNET" />
        <permission name="android.permission.ACCESS_NETWORK_STATE" />
        <permission name="android.permission.READ_PRIVILEGED_PHONE_STATE" />
        <permission name="android.permission.MODIFY_PHONE_STATE" />
        <permission name="android.permission.WRITE_EMBEDDED_SUBSCRIPTIONS" />
        <permission name="com.samsung.permission.HQM_NOTIFICATION_PERMISSION" />
        <permission name="com.sec.android.diagmonagent.permission.DIAGMON" />
        <permission name="com.sec.android.diagmonagent.permission.PROVIDER" />
        <permission name="com.samsung.android.sdm.config.permission.RECEIVE_CONFIG_CHANGED" />
        <permission name="com.samsung.android.sdm.config.provider.READ_OMC_CONFIG" />
        <permission name="com.samsung.android.security.permission.SAMSUNG_KEYSTORE_PERMISSION" />
        <permission name="android.permission.CONNECTIVITY_USE_RESTRICTED_NETWORKS"/>
        <permission name="android.permission.UPDATE_DEVICE_STATS" />
        <permission name="android.permission.SECURE_ELEMENT_PRIVILEGED_OPERATION"/>
        <permission name="android.permission.WRITE_SECURE_SETTINGS"/>

    </privapp-permissions>
</permissions>
